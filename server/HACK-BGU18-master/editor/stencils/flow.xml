<shapes name="Flow">
	<shape name="Node" h="25" w="100" aspect="variable" strokewidth="inherit">
		<connections>
			<constraint x="0" y="0.5" perimeter="0" name="W" />
			<constraint x="1" y="0.5" perimeter="0" name="E" />
		</connections>
		<background>
			<path>
				<move x="0" y="0" />
				<line x="100" y="0" />
				<line x="100" y="25" />
				<line x="0" y="25" />
				<close />
			</path>

		</background>
		<foreground>
			<fillstroke />
		</foreground>
	</shape>
	<shape aspect="fixed" h="50" name="General Start" strokewidth="inherit"
		w="50">
		<connections>
			<constraint name="N" perimeter="0" x="0.5" y="0" />
			<constraint name="S" perimeter="0" x="0.5" y="1" />
			<constraint name="W" perimeter="0" x="0" y="0.5" />
			<constraint name="E" perimeter="0" x="1" y="0.5" />
			<constraint name="NW" perimeter="0" x="0.145" y="0.145" />
			<constraint name="SW" perimeter="0" x="0.145" y="0.855" />
			<constraint name="NE" perimeter="0" x="0.855" y="0.145" />
			<constraint name="SE" perimeter="0" x="0.855" y="0.855" />
		</connections>
		<background>
			<ellipse h="50" w="50" x="0" y="0" />

		</background>
		<foreground>	
		    <fillstroke />
			<text str="node" x="25" y="25" align="center" valign="middle"/>
		
		</foreground>
	</shape>
	<shape name="BSync" h="25" w="100" aspect="variable" strokewidth="inherit">
		<connections>
			<constraint x="0" y="0.5" perimeter="0" name="W" />
			<constraint x="1" y="0.5" perimeter="0" name="E" />
		</connections>
		<background>
			<path>
				<move x="0" y="0" />
				<line x="100" y="0" />
				<line x="100" y="25" />
				<line x="0" y="25" />
				<close />
			</path>
		</background>
		<foreground>
			<fillstroke />
		</foreground>
	</shape>
		<shape name="Counter" h="25" w="100" aspect="variable" strokewidth="inherit">
		<connections>
			<constraint x="0" y="0.5" perimeter="0" name="W" />
			<constraint x="1" y="0.5" perimeter="0" name="E" />
		</connections>
		<background>
			<path>
				<move x="0" y="0" />
				<line x="100" y="0" />
				<line x="100" y="25" />
				<line x="0" y="25" />
				<close />
			</path>
		</background>
		<foreground>
			<fillstroke />
		</foreground>
	</shape>
		<shape name="Moodle" h="25" w="100" aspect="variable" strokewidth="inherit">
		<connections>
			<constraint x="0" y="0.5" perimeter="0" name="W" />
			<constraint x="1" y="0.5" perimeter="0" name="E" />
		</connections>
		<background>
			<path>
				<move x="0" y="0" />
				<line x="100" y="0" />
				<line x="100" y="25" />
				<line x="0" y="25" />
				<close />
			</path>
		</background>
		<foreground>
			<fillstroke />
		</foreground>
	</shape>
</shapes>